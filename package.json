{
  "name": "react-variable-height-infinite-scroller",
  "version": "2.0.7",
  "description": "variable row height scroller that renders just the visible rows with no precomputation of row-height necessary",
  "main": "index.js",
  "scripts": {
    "test": "npm run lint",
    "start": "npm run watch & webpack && webpack-dev-server --port 8081 --devtool eval --progress --colors --content-base",
    "build": "babel InfiniteScroller.js -o index.js",
    "watch": "babel --watch InfiniteScroller.js -o index.js",
    "lint": "eslint ./",
    "patch": "release patch",
    "minor": "release minor",
    "major": "release major"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/tnrich/react-variable-height-infinite-scroller.git"
  },
  "keywords": [
    "react",
    "infinite",
    "variable",
    "row",
    "scroll",
    "scroller",
    "adjustable"
  ],
  "author": "tnrich",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/tnrich/react-variable-height-infinite-scroller/issues"
  },
  "homepage": "https://github.com/tnrich/react-variable-height-infinite-scroller#readme",
  "dependencies": {
    "validate.io-nonnegative-integer-array": "^1.0.1"
  },
  "devDependencies": {
    "babel-core": "^5.8.23",
    "babel-eslint": "^4.1.1",
    "babel-loader": "^5.3.2",
    "eslint": "^1.3.1",
    "eslint-config-airbnb": "^0.0.8",
    "eslint-plugin-babel": "^2.1.1",
    "eslint-plugin-react": "^3.3.0",
    "jsx-loader": "^0.13.2",
    "mt-changelog": "^0.6.1",
    "react": "^0.13.3",
    "react-tools": "^0.13.3",
    "reactify": "^1.1.1",
    "release-script": "^0.2.5",
    "webpack": "^1.12.0",
    "webpack-dev-server": "^1.10.1"
  }
}
